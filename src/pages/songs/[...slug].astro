---
import type { CollectionEntry } from "astro:content";
import {songsCollection} from "../../content/collections";
import SongLayout from '../../layouts/SongLayout.astro';


interface Props {
    entry: CollectionEntry<'songs_ru'>;
}

export async function getStaticPaths() {
    const songs = songsCollection;
    
    return songs.map(entry => {
        return ({
            params: {slug: entry.slug}, props: {entry},
        });
    });
}


const {entry} = Astro.props;

---
<SongLayout song={entry}></SongLayout>